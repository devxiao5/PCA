@model IEnumerable<PCA.Models.Client>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Clients.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.States.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
