@model PCA.Models.DailyReportPicture
@{
    ViewBag.Title = "ViewAttachment";
    Layout = null;
}

@if (Model.Files.Any(f => f.FileType == FileType.Picture))
{
            <img src="~/File?id=@Model.Files.First(f => f.FileType == FileType.Picture).FileId" alt="@Model.Description" />
}
else
{
    <p>No Attachment avaliable.</p>
}